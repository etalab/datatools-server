application:
  assets_bucket: transportgouvfr # dist directory
  public_url: http://localhost:9966
  notifications_enabled: true
  port: 4000
  data:
    mapdb: /home/vincent/dev/datatools-server/data
    gtfs: /home/vincent/dev/datatools-server/data
    editor_mapdb: /home/vincent/dev/datatools-server/data
    regions: /home/vincent/dev/datatools-server/data
    use_s3_storage: false
    s3_region: eu-central-1
    aws_role: arn:aws:iam::${AWS_ACCOUNT_NUMBER}:role/${AWS_ROLE_NAME}
    gtfs_s3_bucket: transportgouvfr
modules:
  enterprise:
    enabled: false
  editor:
    enabled: true
  user_admin:
    enabled: true
  validator:
    enabled: true
  gtfsapi:
    enabled: true
    load_on_fetch: false
    load_on_startup: false
    use_extension: xyz
#    update_frequency: 3600 # in seconds
extensions:
  transitland:
    enabled: true
    api: https://transit.land/api/v1/feeds
  transitfeeds:
    enabled: false
    api: http://api.transitfeeds.com/v1/getFeeds
    key: your-api-key
  datagouvfr:
    enabled: true
    api: http://www.data.gouv.fr/api/1/datasets/?facets=tag&tag=gtfs&page_size=20
